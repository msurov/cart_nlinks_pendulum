\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\dot{p}=-L_{q}+Bu & =\frac{\partial n\left(q\right)}{\partial q}\frac{dq}{dt}\xi+n\left(q\right)\dot{\xi}\\
 & =\frac{\partial n\left(q\right)}{\partial q}M^{-1}p\xi+n\left(q\right)\dot{\xi}\\
 & =\frac{\partial n}{\partial q}M^{-1}n\xi^{2}+n\dot{\xi}\\
\dot{\xi} & =-\frac{B_{\perp}}{B_{\perp}n}\left(\frac{\partial n}{\partial q}M^{-1}n\xi^{2}+L_{q}\right)
